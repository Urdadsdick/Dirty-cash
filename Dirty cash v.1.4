if game.PlaceId == 6804602922 then
    -- Load Orion Library
    local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

    -- Create Window
    local Window = OrionLib:MakeWindow({
        Name = "Boxing Beta Script",
        HidePremium = false,
        SaveConfig = true,
        ConfigFolder = "BoxingBetaConfig"
    })

    -- Global Settings
    local settings = {
        walkSpeed = 16,
        godMode = false,
        onePunch = false,
        noClip = false,
        autoFarm = false,
        autoPickup = false
    }

    -- Utility Functions
    local function setWalkSpeed(speed)
        local character = game.Players.LocalPlayer.Character
        if character then
            local humanoid = character:FindFirstChildOfClass("Humanoid")
            if humanoid then
                humanoid.WalkSpeed = speed
            end
        end
    end

    local function enableGodMode()
        settings.godMode = true
        while settings.godMode do
            local character = game.Players.LocalPlayer.Character
            if character then
                local humanoid = character:FindFirstChildOfClass("Humanoid")
                if humanoid then
                    humanoid.Health = humanoid.MaxHealth
                end
            end
            task.wait(0.1)
        end
    end

    local function disableGodMode()
        settings.godMode = false
    end

    local function onePunchKill()
        while settings.onePunch do
            for _, player in pairs(game.Players:GetPlayers()) do
                if player ~= game.Players.LocalPlayer and player.Character then
                    local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
                    if humanoid then
                        humanoid.Health = 0 -- Instantly kill the player
                    end
                end
            end
            task.wait(0.5) -- Adjust timing to prevent server overload
        end
    end

    local function toggleNoClip(state)
        settings.noClip = state
        local character = game.Players.LocalPlayer.Character
        if character then
            for _, part in pairs(character:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.CanCollide = not state
                end
            end
        end
    end

    -- Player Tab
    local playerTab = Window:MakeTab({
        Name = "Player",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })

    playerTab:AddSlider({
        Name = "Walk Speed",
        Min = 16,
        Max = 100,
        Default = 16,
        Increment = 1,
        Callback = function(value)
            settings.walkSpeed = value
            setWalkSpeed(value)
        end
    })

    playerTab:AddTextBox({
        Name = "Mobile Walk Speed",
        Default = "16",
        Numeric = true,
        Callback = function(value)
            local newSpeed = tonumber(value)
            if newSpeed then
                settings.walkSpeed = newSpeed
                setWalkSpeed(newSpeed)
            end
        end
    })

    playerTab:AddToggle({
        Name = "God Mode",
        Default = false,
        Callback = function(state)
            if state then
                enableGodMode()
            else
                disableGodMode()
            end
        end
    })

    playerTab:AddToggle({
        Name = "NoClip",
        Default = false,
        Callback = function(state)
            toggleNoClip(state)
        end
    })

    -- Trolling Tab
    local trollingTab = Window:MakeTab({
        Name = "Trolling",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })

    trollingTab:AddToggle({
        Name = "One Punch Kill",
        Default = false,
        Callback = function(state)
            settings.onePunch = state
            if state then
                onePunchKill()
            end
        end
    })

    trollingTab:AddToggle({
        Name = "Auto Farm",
        Default = false,
        Callback = function(state)
            settings.autoFarm = state
            while settings.autoFarm do
                -- Add Auto Farm Logic
                task.wait(1)
            end
        end
    })

    trollingTab:AddToggle({
        Name = "Auto Pickup",
        Default = false,
        Callback = function(state)
            settings.autoPickup = state
            while settings.autoPickup do
                -- Add Auto Pickup Logic
                task.wait(1)
            end
        end
    })

    -- Credits Tab
    local creditsTab = Window:MakeTab({
        Name = "Credits",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })

    creditsTab:AddLabel("Developed by yooo0684 on Discord")
    creditsTab:AddButton({
        Name = "Join Discord",
        Callback = function()
            setclipboard("https://discord.gg/DvgnsUp3")
        end
    })

    -- Initialize UI
    OrionLib:Init()
end
