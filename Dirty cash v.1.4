if game.PlaceId == 6804602922 then
    -- Load Orion Library
    local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

    -- Create Window
    local Window = OrionLib:MakeWindow({
        Name = "Boxing Beta Script",
        HidePremium = false,
        SaveConfig = true,
        ConfigFolder = "BoxingBetaConfig"
    })

    -- Global Settings
    local settings = {
        autoFarm = false,
        walkSpeed = 16,
        godMode = false
    }

    -- Utility Functions
    local function setWalkSpeed(speed)
        local character = game.Players.LocalPlayer.Character
        if character then
            local humanoid = character:FindFirstChildOfClass("Humanoid")
            if humanoid then
                humanoid.WalkSpeed = speed
            end
        end
    end

    local function enableGodMode()
        settings.godMode = true
        while settings.godMode do
            local character = game.Players.LocalPlayer.Character
            if character then
                local humanoid = character:FindFirstChildOfClass("Humanoid")
                if humanoid then
                    humanoid.Health = humanoid.MaxHealth
                end
            end
            task.wait(0.1)
        end
    end

    local function disableGodMode()
        settings.godMode = false
    end

    local function autoFarm()
        while settings.autoFarm do
            -- Implement your farming logic here
            local character = game.Players.LocalPlayer.Character
            if character then
                local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
                if humanoidRootPart then
                    -- Example: Move to a dummy to train
                    for _, dummy in pairs(workspace:GetChildren()) do
                        if dummy:IsA("Model") and dummy.Name == "TrainingDummy" then
                            humanoidRootPart.CFrame = dummy.PrimaryPart.CFrame
                            -- Add interaction logic if needed
                        end
                    end
                end
            end
            task.wait(1) -- Adjust delay as needed
        end
    end

    -- Player Tab
    local playerTab = Window:MakeTab({
        Name = "Player",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })

    playerTab:AddSlider({
        Name = "Walk Speed",
        Min = 16,
        Max = 100,
        Default = 16,
        Increment = 1,
        Callback = function(value)
            settings.walkSpeed = value
            setWalkSpeed(value)
        end
    })

    playerTab:AddToggle({
        Name = "God Mode",
        Default = false,
        Callback = function(state)
            if state then
                enableGodMode()
            else
                disableGodMode()
            end
        end
    })

    -- Farming Tab
    local farmingTab = Window:MakeTab({
        Name = "Farming",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })

    farmingTab:AddToggle({
        Name = "Auto Farm",
        Default = false,
        Callback = function(state)
            settings.autoFarm = state
            if state then
                autoFarm()
            end
        end
    })

    -- Credits Tab
    local creditsTab = Window:MakeTab({
        Name = "Credits",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })

    creditsTab:AddLabel("Developed by yooo0684 on Discord")
    creditsTab:AddButton({
        Name = "Join Discord",
        Callback = function()
            setclipboard("https://discord.gg/DvgnsUp3")
        end
    })

    -- Initialize UI
    OrionLib:Init()
end
